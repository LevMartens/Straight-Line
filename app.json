{
  "expo": {
    "name": "react-practice-4",
    "slug": "react-practice-4",
    "scheme": "straight-line-mission",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/straight-line-logo-app-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/straight-line-logo-app-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/images/straight-line-logo-app-icon.png"
    }
  }
}
